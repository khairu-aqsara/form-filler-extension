{
  "manifest_version": 3,
  "name": "Form Filler",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "contextMenus",
    "notifications",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "locales.js",
      "faker.js",
      "content.js"
    ]
  }],
  "action": {
    "default_icon": "slack.png"
  },
  "icons": {
    "48": "slack.png",
    "128": "slack.png"
  }
}
